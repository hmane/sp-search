{
  "name": "sp-search",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0 < 23.0.0"
  },
  "main": "lib/index.js",
  "sideEffects": [
    "*.scss",
    "*.css",
    "**/node_modules/@pnp/**",
    "**/node_modules/spfx-toolkit/lib/utilities/context/pnpImports/**",
    "**/node_modules/devextreme/dist/css/**"
  ],
  "scripts": {
    "build": "gulp bundle",
    "build:ship": "gulp bundle --ship",
    "clean": "gulp clean && gulp clean-cache",
    "package": "gulp package-solution --ship",
    "serve": "fast-serve",
    "serve:legacy": "gulp serve",
    "release": "gulp clean && gulp bundle --ship && gulp package-solution --ship",
    "stats": "ANALYZE=1 gulp bundle --ship && gulp analyze-bundle",
    "test": "gulp test",
    "test:jest": "jest --config jest.config.js"
  },
  "dependencies": {
    "@fluentui/react": "^8.106.4",
    "@microsoft/sp-component-base": "1.21.1",
    "@microsoft/sp-core-library": "1.21.1",
    "@microsoft/sp-http": "1.21.1",
    "@microsoft/sp-lodash-subset": "1.21.1",
    "@microsoft/sp-office-ui-fabric-core": "1.21.1",
    "@microsoft/sp-property-pane": "1.21.1",
    "@microsoft/sp-webpart-base": "1.21.1",
    "@pnp/logging": "^4.16.0",
    "@pnp/queryable": "^3.20.1",
    "@pnp/sp": "^3.20.1",
    "@pnp/spfx-controls-react": "^3.22.0",
    "devextreme": "^22.2.3",
    "devextreme-react": "^22.2.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^7.45.4",
    "spfx-toolkit": "file:../spfx-toolkit",
    "tslib": "2.3.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.21.1",
    "@microsoft/eslint-plugin-spfx": "1.21.1",
    "@microsoft/rush-stack-compiler-5.3": "0.1.0",
    "@microsoft/sp-build-web": "1.21.1",
    "@microsoft/sp-module-interfaces": "1.21.1",
    "@rushstack/eslint-config": "4.0.1",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/webpack-env": "~1.15.2",
    "ajv": "^6.12.5",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gulp": "4.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "spfx-fast-serve-helpers": "~1.21.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
